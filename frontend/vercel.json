{
  "version": 2,
  "routes": [
    { "src": "^/pages/product$", "dest": "/product.html" },
    { "src": "^/pages/products$", "dest": "/products.html" },
    { "src": "^/pages/cart$", "dest": "/cart.html" },
    { "src": "^/pages/wishlist$", "dest": "/wishlist.html" },
    { "src": "^/pages/bmi$", "dest": "/bmi.html" },
    { "src": "^/pages/tryon$", "dest": "/tryon.html" },
    { "src": "^/pages/shippinginfo$", "dest": "/shippinginfo.html" },
    { "src": "^/pages/order$", "dest": "/order.html" },
    { "src": "^/pages/experience$", "dest": "/experience.html" },

    { "src": "^/static/(.*)$", "dest": "/static/$1" },

    { "src": "^/$", "dest": "/index.html" }
  ]
}
